{
    "name": "cleaniquecoders/blueprint-macro",
    "description": "Laravel Blueprint Macro",
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\Blueprint\\Macro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\Blueprint\\Macro\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/filesystem": "^9.0",
        "illuminate/support": "^9.0",
        "illuminate/auth": "^9.0",
        "doctrine/dbal": "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "7.*",
        "mockery/mockery": "^1.1",
        "pestphp/pest": "^1.21",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\Blueprint\\Macro\\BlueprintMacroServiceProvider"
            ]
        }
    }
}
